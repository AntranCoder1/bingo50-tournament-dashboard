{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "object-literal-sort-keys": false,
        "no-console": false,
        "prefer-for-of": false,
        "no-var-requires": 0,
        "import-spacing": false,
        "max-line-length": [
            true, 
            {
              "limit": 250, 
              "ignore-pattern": "^import [^,]+ from |^export | implements"
            }
          ]
    },
    "rulesDirectory": []
}
